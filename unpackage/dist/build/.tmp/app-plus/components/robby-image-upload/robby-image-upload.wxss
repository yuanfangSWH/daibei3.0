
.imageUploadContainer{
	padding: 10rpx 5rpx;
	margin: 10rpx 5rpx;
}
.dragging{
	-webkit-transform: scale(1.2);
	    -ms-transform: scale(1.2);
	        transform: scale(1.2)
}
.imageUploadList{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.imageItem{ position: relative;}
.imageItem, .imageUpload{
	width: 216rpx;
	height: 216rpx;
	margin: 0 10rpx 10rpx 0;
}
.imageDel{
	position: absolute;
	right: 10rpx;
	top: 10rpx;
	background-color: rgba(0,0,0,0.5);
	width: 36rpx;
	text-align: center;
	line-height: 35rpx;
	border-radius: 17rpx;
	color: white;
	font-size: 30rpx;
	padding-bottom: 2rpx;
}
.imageItem image, .moveImage{
	width: 216rpx;
	height: 216rpx;
}
.imageUpload{
	line-height: 180rpx;
	text-align: center;
	font-size: 148rpx;font-family:PingFang-SC-Medium;
	color: #D9D9D9;
	border: 4rpx dashed #D9D9D9; -webkit-box-sizing: border-box; box-sizing: border-box;
}
.moveImage{
	position: absolute;
}

